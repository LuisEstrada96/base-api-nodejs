{
	"name": "its-base-api",
	"description": "ITS Base API",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": ">=14.15.0",
		"npm": ">=6.14.8"
	},
	"scripts": {
		"dev": "nodemon base-api.js --watch",
		"start": "node base-api.js",
		"qa": "pm2 start ecosystem.config.js --env qa",
		"prod": "pm2 start ecosystem.config.js --env prod",
		"restart": "pm2 restart base-api",
		"logs": "pm2 logs base-api --lines 100"
	},
	"dependencies": {
		"body-parser": "1.19.0",
		"config": "3.3.2",
		"cors": "2.8.5",
		"dotenv": "8.2.0",
		"express": "4.17.1",
		"express-mung": "0.5.1",
		"fcm-node": "1.5.2",
		"lodash": "4.17.20",
		"moment": "2.29.1",
		"mongodb": "3.6.2",
		"morgan": "1.10.0",
		"nodemon": "2.0.6",
		"triple-beam": "1.3.0",
		"winston": "3.3.3"
	},
	"devDependencies": {
		"nodemon": "2.0.4"
	}
}
