{
  "name": "its-base-api",
  "description": "ITS Base API",
  "version": "0.1.0",
  "private": true,
  "main": "base-api.js",
  "scripts": {
    "dev": "NODE_ENV=dev nodemon base-api.js --watch resources",
    "start": "node base-api.js",
    "forever": "NODE_ENV=production forever -a --uid base-api start base-api.js",
    "restart": "forever restart base-api"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "config": "3.2.3",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-mung": "0.5.1",
    "lodash": "4.17.15",
    "morgan": "1.9.1",
    "triple-beam": "1.3.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "nodemon": "1.19.4"
  }
}
